      },
      "title": "Documentationmaterial",
      "type": "array"
    },
    "records": {
      "items": {
        "type": "string"
      },
      "title": "Records",
      "type": "array"
    },
    "collections": {
      "items": {
        "type": "string"
      },
      "title": "Collections",
      "type": "array"
    }
  },
  "required": [
    "id",
    "pid",
    "name",
    "accessRights",
    "legalInfo",
    "howToCite",
    "typeOfData",
    "dateCreated",
